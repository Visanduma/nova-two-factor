{
    "name": "visanduma/nova-two-factor",
    "description": "Nova Two Factor Authentication",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bacon/bacon-qr-code": "^3.0",
        "pragmarx/google2fa-laravel": "^2.0",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Visanduma\\NovaTwoFactor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Visanduma\\NovaTwoFactor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
